<?xml version='1.0' encoding='utf-8'?>
<widget id="puka.eyetest.com" version="0.0.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="AdMob">
        <param name="android-package" value="com.rjfun.cordova.admob.AdMobPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Screenshot">
        <param name="android-package" value="com.darktalker.cordova.screenshot.Screenshot" />
    </feature>
    <feature name="GooglePlayGame">
        <param name="android-package" value="com.a42.cordova.plugins.GooglePlayGame" />
    </feature>
    <feature name="SocialSharing">
        <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Media">
        <param name="android-package" value="org.apache.cordova.media.AudioHandler" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="AdBuddizBridge">
        <param name="android-package" value="com.purplebrain.adbuddiz.sdk.AdBuddizBridge" />
    </feature>
    <name>Eye Test</name>
    <description>
        Free test your colour vision
    </description>
    <author email="npkhanh2311@gmail.com" href="http://cordova.io">
        Khanh Nguyen Phuc
    </author>
    <content src="index.html" />
    <preference name="orientation" value="portrait" />
    <access origin="*" />
</widget>
